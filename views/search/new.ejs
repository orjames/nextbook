<h1>Search</h1>

<form action="/projects" method="POST">
  <div class="form-group">
    <label for="name">Book Name</label>
    <input type="text" class="form-control" id="name" name="name" required>
  </div>

  <div class="form-group">
    <label for="githubLink">Author</label>
    <input type="text" class="form-control" id="githubLink" name="githubLink" required>
  </div>

  <div class="form-group">
    <label for="deployedLink">Genre</label>
    <input type="text" class="form-control" id="deployedLink" name="deployedLink" required>
  </div>

  <input type="submit" class="btn btn-primary">
</form>